Require Import Naturelle.
Section Session1_2019_Logique_Exercice_1.

Variable A B C: Prop.

Theorem Exercice_1_Naturelle :  (A -> B) -> (((~(C)) -> (~(B))) -> (A -> C)).
Proof.
I_imp H1.
I_imp H2.
I_imp HA. 
absurde HC.
I_antiT B.
E_imp A.
Hyp H1.
Hyp HA.
E_imp (~C).
Hyp H2.
Hyp HC.

(* A COMPLETER *)
Qed.

Theorem Exercice_1_Coq :  (A -> B -> C) -> ((A /\ B) -> C).
Proof.
(* A COMPLETER *)
Qed.

End Session1_2019_Logique_Exercice_1.

